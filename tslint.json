{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [
      false,
      "spaces",
      2
    ],
    "interface-name": false,
    "variable-name": false,
    "typedef-whitespace": false,
    "prefer-const": false,
    "no-unused-vars": 0,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "trailing-comma": false,
    "member-access": false,
    "no-empty": false,
    "no-trailing-whitespace": false,
    "no-console": false,
    "no-shadowed-variable": false,
    "member-ordering": false,
    "semicolon": false,
    "array-type": false,
    "comment-format": false,
    "curly": false,
    "eslint-plugin-vue": false,
    "disable-next-line": false,
    "max-classes-per-file": 10,
    "max-line-length": 180,
    "noImplicitAny": false,
    "strict": false,
    "quotemark": [
      false,
      "single"
    ]
  }
}
